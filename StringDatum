using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


namespace datum
{
    

    class Program
      
    {
        static void Main(string[] args)           

        {
            DateTime time = DateTime.Now;
            string format = "yyyyMMdd";
            string datum = (time.ToString(format));
            Console.WriteLine(datum);
            Console.ReadLine();
        }
    }
}
    
